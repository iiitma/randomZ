{
  "name": "js-randomize",
  "version": "2.0.0",
  "description": "A simple javascript utility to generate random values in your code",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "start": "tsup --watch",
    "release": "npx -p @semantic-release/changelog -p @semantic-release/git -p semantic-release semantic-release"
  },
  "keywords": [
    "random",
    "randomize",
    "typescript",
    "random generator",
    "utility"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loveakinlesi/js-randomize.git"
  },
  "author": "Love Akinlesi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loveakinlesi/js-randomize/issues"
  },
  "homepage": "https://github.com/loveakinlesi/js-randomize#readme",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "jest": "^29.7.0",
    "semantic-release": "24.1.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
